---
import Layout from '../../layouts/Layout.astro';
import ImplantesRC from '../../components/Implantes.astro';

import { getI18N } from "../../i18n";
const { currentLocale } = Astro;
const i18n = await getI18N({ currentLocale })
---

<Layout title={i18n.SERVICE_IMPLANTS}>
    <ImplantesRC title={i18n.SERVICE_IMPLANTS} subtitle1={i18n.IMPLANTS_SUBTITLE_1} text1={i18n.IMPLANTS_TEXT_1} subtitle2={i18n.IMPLANTS_SUBTITLE_2} text2={i18n.IMPLANTS_TEXT_2} subtitle3={i18n.IMPLANTS_SUBTITLE_3} text3={i18n.IMPLANTS_TEXT_3} subtitle4={i18n.IMPLANTS_SUBTITLE_4} text4={i18n.IMPLANTS_TEXT_4} />
</Layout>